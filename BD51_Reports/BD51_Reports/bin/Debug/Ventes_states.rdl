  <DataSets>
    <DataSet Name="Ventes_par_state">
      <Query>
        <DataSourceName>EMODE_BD51</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@YEAR">
            <Value>=Parameters!YEAR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT       
	 OL.STATE
	 ,SUM(SF.AMOUNT_SOLD) AS Amout
FROM           
	SHOP_FACTS AS SF INNER JOIN CALENDAR_YEAR_LOOKUP AS CAL 
	ON SF.WEEK_KEY = CAL.WEEK_KEY 
	INNER JOIN OUTLET_LOOKUP AS OL 
	ON OL.SHOP_CODE = SF.SHOP_CODE
WHERE
	CAL.YEAR = @YEAR
GROUP BY 
	OL.STATE
ORDER BY 
	1
;</CommandText>
      </Query>
      <Fields>
        <Field Name="STATE">
          <DataField>STATE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="amout">
          <DataField>Amout</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
