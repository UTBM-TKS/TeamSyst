  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>EMODE_BD51</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        O.STATE, O.SHOP_NAME, C.YEAR, SUM(S.AMOUNT_SOLD) AS Total_amount, SUM(S.QUANTITY_SOLD) AS Total_quantity, C.QUARTER, O.CITY, A.CATEGORY, A.FAMILY_NAME
FROM            OUTLET_LOOKUP AS O INNER JOIN
                         SHOP_FACTS AS S ON O.SHOP_CODE = S.SHOP_CODE INNER JOIN
                         CALENDAR_YEAR_LOOKUP AS C ON C.WEEK_KEY = S.WEEK_KEY INNER JOIN
                         ARTICLE_LOOKUP AS A ON A.ARTICLE_CODE = S.ARTICLE_CODE
WHERE C.YEAR = @Year
GROUP BY O.STATE, O.SHOP_NAME, C.YEAR, C.QUARTER, O.CITY, A.CATEGORY, A.FAMILY_NAME
ORDER BY C.YEAR, O.SHOP_NAME</CommandText>
      </Query>
